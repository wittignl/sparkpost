{
  "name": "sparkpost",
  "version": "2.1.4",
  "description": "An updated version of the SparkPost client library for Node.js",
  "type": "module",
  "packageManager": "pnpm@10.10.0",
  "keywords": [
    "email",
    "messaging"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wittignl/sparkpost"
  },
  "author": "Message Systems, Inc.",
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Wittig B.V.",
      "email": "info@wittig.nl",
      "url": "https://www.wittig.nl/"
    }
  ],
  "bugs": {
    "url": "https://github.com/wittignl/sparkpost/issues"
  },
  "homepage": "https://github.com/wittignl/sparkpost",
  "engines": {
    "node": ">=12"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "test": "vitest run"
  },
  "dependencies": {
    "es-toolkit": "^1.37.2",
    "undici": "^7.9.0"
  },
  "devDependencies": {
    "@rslib/core": "^0.6.9",
    "@types/node": "^22.8.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "@total-typescript/tsconfig": "^1.0.4"
  }
}
