{
  "name": "@wittignl/sparkpost",
  "version": "1.0.1",
  "description": "An updated version of the SparkPost client library for Node.js",
  "type": "module",
  "packageManager": "pnpm@10.10.0",
  "keywords": [
    "email",
    "messaging"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wittignl/sparkpost"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": {
    "name": "Wittig",
    "email": "info@wittig.nl",
    "url": "https://www.wittig.nl/"
  },
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Message Systems, Inc."
    },
    {
      "name": "Wittig B.V.",
      "email": "info@wittig.nl",
      "url": "https://www.wittig.nl/"
    }
  ],
  "bugs": {
    "url": "https://github.com/wittignl/sparkpost/issues"
  },
  "homepage": "https://github.com/wittignl/sparkpost",
  "engines": {
    "node": ">=12"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "test": "vitest run",
    "lint": "biome lint --write",
    "format": "dprint fmt",
    "release": "semantic-release",
    "prepack": "pnpm run build"
  },
  "dependencies": {
    "es-toolkit": "^1.37.2",
    "undici": "^7.9.0"
  },
  "devDependencies": {
    "@rslib/core": "^0.6.9",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.8.1",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "semantic-release": "^24.2.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "@types/sparkpost": "^2.1.8"
  }
}
